    "version": "0.2.0",
    "configurations": [
        {
            "name": "opsml app",
            "type": "python",
            "request": "launch",
            "module": "gunicorn",
            "args": [
                "-k uvicorn.workers.UvicornWorker",
                "--bind=0.0.0.0:8889",
                "opsml.app.main:run_app()"
            ],
            "jinja": true,
            "justMyCode": true,
            "env": {
                "OPSML_TRACKING_URI":"sqlite:///tmp.db",
                "OPSML_STORAGE_URI":"./mlruns"

              }
        }
    ]
}